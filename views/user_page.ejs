        .form-group {
            margin-bottom: 15px;
        }
        label {
            display: block;
            margin-bottom: 5px;
            font-weight: bold;
            color: #555;
        }
        span {
            display: block;
            margin-bottom: 3px;
            font-weight: normal; /* Убрали жирный шрифт */
            color: #888; /* Более светлый серый цвет */
            font-size: 12px; /* Уменьшили размер шрифта */
        }

        input[type="text"],
        input[type="file"],
        input[type="number"],
        textarea {
            width: 100%;
            padding: 10px;
            border: 1px solid #ccc;
            border-radius: 4px;
            font-size: 14px;
        }
        textarea {
            resize: none;
        }
        button {
            background-color: #0298ea;
            color: white;
            border: none;
            padding: 10px;
            border-radius: 4px;
            cursor: pointer;
            font-size: 16px;
            width: 100%;
        }
        button:hover {
            background-color: #027aba; /* Темнее при наведении */
        }
        .file-upload {
            display: flex; /* Используем Flexbox */
            flex-direction: column; /* Размещаем элементы по вертикали */
            align-items: center; /* Центрируем по горизонтали */
            position: relative; /* Сравниваем положение относительно ближайшего родителя */
        }

        .file-upload input {
            display: none; /* Скрываем оригинальный input */
        }

        .upload-label {
            display: flex;
            align-items: center;
            justify-content: center;
            width: 50px; /* Ширина кружка */
            height: 50px; /* Высота кружка */
            border-radius: 50%; /* Закругляем углы для создания кружка */
            background-color: #f0f0f0; /* Цвет фона кружка */
            border: 2px solid #2d3033; /* Цвет бордюра кружка */
            cursor: pointer; /* Курсор-указатель при наведении */
            transition: background-color 0.3s; /* Плавный переход цвета */
            margin-top: 10px; /* Отступ сверху чтобы отделить от input */
        }

        .upload-label:hover {
            background-color: #e0e0e0; /* Цвет при наведении */
        }

        .upload-icon {
            font-size: 24px; /* Размер иконки */
            color: #2d3033; /* Цвет иконки */
        }


    <section id="intro" class="contents-container">
        <article class="flex flex-col items-center w-full max-w-screen-md p-4">
            <div id="text-group" class="text-center mb-5">
                <h2 style="line-height: 1.1" class="text-4xl font-extrabold tracking-tight text-gray-900 dark:text-white md:text-3xl md:text-6xl">
                    Mint your token
                </h2>
            </div>
            <div class="w-full md:w-fit mx-auto">
                <form id="data-form">
                    <div class="form-group mb-4">
                        <div class="file-upload">
                        <input type="file" id="photo" accept="image/*" required>
                        <label for="photo" class="upload-label">
                            <i class="fas fa-pencil-alt upload-icon"></i>
                        </label>
                    </div>        
                    </div>                                   
                    <div class="form-group mb-4">
                        <label for="name">Token name</label>
                        <input type="text" placeholder="Bitcoin" id="name" required>
                        <span>Your project unabbreviated name with spaces (usually 1-3 words).</span>
                    </div>
                    <div class="form-group mb-4">
                        <label for="symbol">Token symbol</label>
                        <input type="text" placeholder="BTC" id="symbol" required>
                        <span>Currency symbol appearing in balance (usually 3-5 uppercase chars).</span>
                    </div>
                    <div class="form-group mb-4">
                        <label for="precision">Token Decimals</label>
                        <input type="number" value="9" id="precision" required>
                        <span>The decimal precision of your token (9 is TON default).</span>
                    </div>
                    <div class="form-group mb-4">
                        <label for="amount">Tokens to mint</label>
                        <input type="number" placeholder="1000" id="amount" required>
                        <span>Number of initial tokens to mint and send to your wallet address (float).</span>
                    </div>
                    <div class="form-group mb-4">
                        <label for="description">Description</label>
                        <input type="text" id="description" placeholder="The best token ever" required>
                        <span>Optional sentence explaining about your project.</span>
                    </div>
                    <button onclick="createJettonButton()" style="
                        background-color: #007bff; /* Начальный цвет кнопки */
                        color: white; /* Цвет текста */
                        padding: 10px 20px; /* Отступы для кнопки */
                        border: none; /* Убираем рамки */
                        border-radius: 5px; /* Закругляем углы */
                        font-size: 16px; /* Размер шрифта */
                        cursor: pointer; /* Изменение курсора при наведении */
                        transition: background-color 0.3s, transform 0.3s; /* Плавный переход для фона и масштабирования */
                    " 
                    onmouseover="this.style.backgroundColor='#0056b3'; this.style.transform='scale(1.1)';" 
                    onmouseout="this.style.backgroundColor='#007bff'; this.style.transform='scale(1)';">
                        Deploy
                    </button>
                </form>
            </div>
